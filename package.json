{
  "name": "template-aws-sls",
  "version": "0.0.1",
  "description": "AWS + Serverless API for your application",
  "main": "index.js",
  "scripts": {
    "predeploy:dev": "node --trace-warnings scripts/init-deploy.js dev",
    "deploy:dev": "sls deploy --stage dev --config serverless.ts --verbose",
    "free-up:dev": "sls deploy --stage dev --config serverless.free-up-resources.ts --verbose",
    "predeploy:prod": "node scripts/init-deploy.js prod",
    "deploy:prod": "sls deploy --stage prod --config serverless.ts --verbose",
    "predeploy:test": "node scripts/init-deploy.js test",
    "deploy:test": "sls deploy --stage test --config serverless.ts --verbose",
    "free-up:test": "sls deploy --stage test --config serverless.free-up-resources.ts --verbose",
    "deploy:local": "sls offline start --stage local --config serverless.ts --noAuth",
    "sls:package:dev": "sls package --stage dev --config serverless.ts",
    "sls:package:test": "sls package --stage test --config serverless.ts",
    "sls:package:prod": "sls package --stage prod --config serverless.ts",
    "sls:print:dev": "sls print --stage dev --config serverless.ts --format json",
    "sls:print:test": "sls print --stage dev --config serverless.ts",
    "sls:print:prod": "sls print --stage prod --config serverless.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest --watchAll --colors --coverage",
    "test:ci": "CI=true jest --ci --colors --coverage --passWithNoTests",
    "sonarqube:up": "docker-compose -f docker-compose.yml up -d sonarqube",
    "sonarqube-scanner": "node sonar-project.js",
    "sonarqube-verify": "sonarqube-verify",
    "containers:down": "docker-compose -f docker-compose.yml down"
  },
  "engines": {
    "node": "20",
    "npm": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/workflow-intelligence-nexus/win.sls"
  },
  "author": "Workflow Intelligence Nexus",
  "contributors": [
    {
      "name": "Andrey Zaikin",
      "email": "azaikin@flo.team",
      "url": "https://github.com/zaikin-andrew"
    },
    {
      "name": "Anastasia Zhivaeva",
      "email": "azhivaeva@flo.team",
      "url": "https://github.com/azhivaeva-flo"
    },
    {
      "name": "Khoroshilov Kirill",
      "email": "khoroshilov@flo.team",
      "url": "https://github.com/khoroshilov-flo"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/workflow-intelligence-nexus/win.sls/issues"
  },
  "homepage": "https://github.com/workflow-intelligence-nexus/win.sls#readme",
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@redtea/serverless-env-generator": "3.0.1",
    "@serverless/typescript": "3.38.0",
    "@types/aws-lambda": "8.10.134",
    "@types/bluebird": "^3.5.42",
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "deepmerge": "4.3.1",
    "esbuild": "0.20.1",
    "esbuild-node-externals": "1.13.0",
    "esbuild-plugin-tsc": "^0.4.0",
    "esbuild-visualizer": "0.6.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "pretty-quick": "4.0.0",
    "serverless": "3.38.0",
    "serverless-esbuild": "1.51.0",
    "serverless-offline": "13.3.3",
    "serverless-offline-sns": "0.77.2",
    "serverless-offline-sqs": "8.0.0",
    "serverless-prune-plugin": "2.0.2",
    "serverless-step-functions": "^3.19.0",
    "sonarqube-scanner": "^3.3.0",
    "sonarqube-verify": "^1.0.2",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "3.515.0",
    "@aws-sdk/credential-providers": "3.515.0",
    "@flopackages/errors": "^1.1.0",
    "@redtea/format-axios-error": "2.1.1",
    "@types/node": "20.11.19",
    "@types/validator": "^13.11.9",
    "@workflowwin/iconik-api": "2.2.82",
    "axios": "1.6.7",
    "dotenv": "16.4.5",
    "node-cache": "5.1.2",
    "reflect-metadata": "^0.2.1",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
